/* Zadanie nr 2: Poslugujac sie operatorem "reszta z dzielenia", napisz program, ktory wypisywal bedzie na ekranie kolejne liczby calkowite, poczawszy od 1 do 100,
a po zakonczeniu kazdej dziesiatki( 10, 20, 30, ...) wypisze tekst "zakonczona dziesiatka" */

#include "pch.h"
#include <iostream>

using namespace std;

int main()
{
	for (int i = { 1 }; i <= 100; i++)
	{
		cout << i << endl;
		if (i % 10 == 0)
		{
			cout << "zakonczona dziesiatka" << endl;
		}
	}
}
