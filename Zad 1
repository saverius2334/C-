/* Zadanie nr 1: Napisz program, który z pomocą jednej instrukcji "cout" napisze na ekranie następujący tekst:
 witamy
 na
 p
 o
 kladzie */

#include "pch.h"
#include <iostream>

using namespace std;

int main()
{
	cout << "witamy\nna\np\no\nkladzie\n";
	cout << "witamy" << endl << "na" << endl << "p" << endl << "o" << endl << "kladzie";
}
